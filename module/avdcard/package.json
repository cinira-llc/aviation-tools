{
  "name": "@cinira-llc/avdcard",
  "description": "Avionics data card utilities",
  "homepage": "https://github.com/cinira-llc/avionics-tools#readme",
  "bin": {
    "credstore": "dist/avdcard.js"
  },
  "bugs": {
    "url": "https://github.com/cinira-llc/avionics-tools/issues"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=22.0.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cinira-llc/avionics-tools.git"
  },
  "license": "MIT",
  "author": "Matt Jensen",
  "main": "dist/avdcard.js",
  "scripts": {
    "build": "tsc --project tsconfig-build.json",
    "clean": "rm -rf ./build ./coverage ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "test": "jest"
  },
  "devDependencies": {
    "@semantic-release/exec": "^7.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {

  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/github",
      "@semantic-release/npm",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "echo -n '${nextRelease.version}' > ./dist/VERSION"
        }
      ]
    ]
  }
}
